{
  "name": "@clduab11/gemini-flow",
  "version": "1.1.0",
  "description": "Gemini Flow - Simple AI assistant CLI powered by Google Gemini",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "gemini-flow": "bin/gemini-flow",
    "gf": "bin/gemini-flow"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli/index.js",
      "types": "./dist/cli/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "bin/**/*",
    "config/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && npm run build:cli",
    "build:cli": "chmod +x bin/gemini-flow && chmod +x dist/cli/index.js",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules\" jest",
    "test:lightweight": "GEMINI_FLOW_MODE=minimal npm test",
    "test:enterprise": "GEMINI_FLOW_MODE=enterprise npm test",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "dev": "tsx watch src/index.ts",
    "dev:lightweight": "GEMINI_FLOW_MODE=minimal tsx watch src/index.ts",
    "dev:enterprise": "GEMINI_FLOW_MODE=enterprise tsx watch src/index.ts",
    "cli:dev": "tsx src/cli/index.ts",
    "start": "node dist/index.js",
    "start:lightweight": "GEMINI_FLOW_MODE=minimal node dist/index.js",
    "start:enterprise": "GEMINI_FLOW_MODE=enterprise node dist/index.js",
    "clean": "rm -rf dist coverage",
    "doctor": "node dist/cli/index.js doctor",
    "feature:enable": "node -e \"console.log('Use: gemini-flow config --set features.{feature}=true')\"",
    "feature:status": "node dist/cli/index.js config --get features",
    "install:enterprise": "npm install @google-cloud/vertexai googleapis google-auth-library better-sqlite3",
    "install:workspace": "npm install googleapis google-auth-library",
    "install:memory": "npm install better-sqlite3 sqlite3",
    "install:mcp": "npm install @modelcontextprotocol/sdk",
    "install:full": "npm install @google-cloud/vertexai @google-cloud/aiplatform googleapis google-auth-library better-sqlite3 sqlite3 @modelcontextprotocol/sdk",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "gemini",
    "ai",
    "google",
    "cli",
    "assistant",
    "generative-ai",
    "conversation",
    "chat"
  ],
  "author": "Gemini-Flow Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clduab11/gemini-flow.git"
  },
  "bugs": {
    "url": "https://github.com/clduab11/gemini-flow/issues"
  },
  "homepage": "https://github.com/clduab11/gemini-flow#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "inquirer": "^9.2.0",
    "ora": "^7.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.8",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "node": ">=18.0.0"
  },
  "optionalDependencies": {
    "@google-cloud/vertexai": "^1.4.0",
    "@google-cloud/aiplatform": "^3.12.0",
    "google-auth-library": "^9.4.0",
    "googleapis": "^131.0.0",
    "better-sqlite3": "^9.2.2",
    "sqlite3": "^5.1.6",
    "sql.js": "^1.8.0",
    "@modelcontextprotocol/sdk": "^0.1.0"
  },
  "geminiFlow": {
    "mode": "dual",
    "features": {
      "sqliteAdapters": {
        "enabled": false,
        "mode": "auto",
        "fallback": true
      },
      "vertexAi": {
        "enabled": false,
        "mode": "auto",
        "dependencies": ["@google-cloud/vertexai", "@google-cloud/aiplatform"]
      },
      "googleWorkspace": {
        "enabled": false,
        "mode": "auto",
        "dependencies": ["googleapis", "google-auth-library"]
      },
      "deepmind": {
        "enabled": false,
        "mode": "auto",
        "experimental": true
      },
      "a2aProtocol": {
        "enabled": false,
        "mode": "auto"
      },
      "mcpProtocol": {
        "enabled": false,
        "mode": "auto",
        "dependencies": ["@modelcontextprotocol/sdk"]
      },
      "quantumHybrid": {
        "enabled": false,
        "mode": "manual",
        "experimental": true
      },
      "neuralPatterns": {
        "enabled": false,
        "mode": "auto",
        "experimental": true
      },
      "swarmOrchestration": {
        "enabled": false,
        "mode": "auto"
      },
      "distributedMemory": {
        "enabled": false,
        "mode": "auto"
      },
      "wasmOptimization": {
        "enabled": true,
        "mode": "auto"
      },
      "connectionPooling": {
        "enabled": true,
        "mode": "auto"
      },
      "caching": {
        "enabled": true,
        "mode": "auto"
      }
    },
    "installers": {
      "enterprise": "npm install @google-cloud/vertexai googleapis google-auth-library better-sqlite3",
      "workspace": "npm install googleapis google-auth-library",
      "memory": "npm install better-sqlite3 sqlite3",
      "mcp": "npm install @modelcontextprotocol/sdk",
      "full": "npm install @google-cloud/vertexai @google-cloud/aiplatform googleapis google-auth-library better-sqlite3 sqlite3 @modelcontextprotocol/sdk"
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}