{
  "name": "@clduab11/gemini-flow",
  "version": "1.0.4",
  "description": "Gemini Flow - Revolutionary Multi-Model AI Orchestration Platform powered by Google Gemini with quantum computing capabilities",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "quantum-flow": "bin/gemini-flow",
    "qf": "bin/gemini-flow",
    "gemini-flow": "bin/gemini-flow",
    "gf": "bin/gemini-flow"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli/index.js",
      "types": "./dist/cli/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "bin/**/*",
    "config/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && npm run build:cli",
    "build:cli": "chmod +x bin/gemini-flow && chmod +x dist/cli/index.js",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules\" jest",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "dev": "tsx watch src/index.ts",
    "cli:dev": "tsx src/cli/index.ts",
    "start": "node dist/index.js",
    "benchmark": "tsx scripts/benchmark.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "postinstall": "echo 'Gemini-Flow installed successfully'",
    "verify-api-key": "node scripts/verify-api-key.js",
    "setup": "cp .env.example .env && echo 'Please edit .env with your API keys' && npm run verify-api-key"
  },
  "keywords": [
    "quantum-computing",
    "qiskit",
    "pennylane",
    "multi-paradigm",
    "quantum-ai",
    "classical-quantum-hybrid",
    "quantum-machine-learning",
    "quantum-optimization",
    "variational-quantum-eigensolver",
    "quantum-supremacy",
    "gemini",
    "ai",
    "orchestration",
    "swarm",
    "agents",
    "google",
    "workspace",
    "mcp",
    "cli",
    "monorepo"
  ],
  "author": "Gemini-Flow Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clduab11/gemini-flow.git"
  },
  "bugs": {
    "url": "https://github.com/clduab11/gemini-flow/issues"
  },
  "homepage": "https://github.com/clduab11/gemini-flow#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "dependencies": {
    "@google-cloud/aiplatform": "^3.0.0",
    "@google-cloud/vertexai": "^1.10.0",
    "@google/generative-ai": "^0.24.1",
    "@modelcontextprotocol/sdk": "^1.17.1",
    "@types/better-sqlite3": "^7.6.13",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "glob": "^10.3.0",
    "googleapis": "^154.1.0",
    "inquirer": "^9.2.0",
    "ora": "^7.0.1",
    "source-map-support": "^0.5.21",
    "sql.js": "^1.13.0",
    "sqlite3": "^5.1.6",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@types/better-sqlite3": "^7.6.11",
    "@types/glob": "^8.1.0",
    "@types/inquirer": "^9.0.8",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.1.0",
    "@types/sql.js": "^1.4.9",
    "@types/sqlite3": "^3.1.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-jest": "^30.0.5",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "node": ">=18.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "optionalDependencies": {
    "better-sqlite3": "^12.2.0"
  }
}