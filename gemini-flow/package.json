{
  "name": "gemini-flow",
  "version": "2.0.0-alpha",
  "description": "AI orchestration platform powered by Google Gemini with 64 specialized agents",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "gemini-flow": "bin/gemini-flow",
    "gf": "bin/gemini-flow"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli/index.js",
      "types": "./dist/cli/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "bin/**/*",
    "config/**/*",
    "README.md",
    "LICENSE"
  ],
  "workspaces": [
    "packages/*",
    "packages/agents/*",
    "packages/core/*",
    "packages/integrations/*"
  ],
  "scripts": {
    "build": "tsc && npm run build:cli",
    "build:cli": "chmod +x bin/gemini-flow && chmod +x dist/cli/index.js",
    "build:workspaces": "npm run build --workspaces --if-present",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules\" jest",
    "test:workspaces": "npm run test --workspaces --if-present",
    "lint": "eslint src/**/*.ts",
    "lint:workspaces": "npm run lint --workspaces --if-present",
    "typecheck": "tsc --noEmit",
    "typecheck:workspaces": "npm run typecheck --workspaces --if-present",
    "dev": "tsx watch src/index.ts",
    "cli:dev": "tsx src/cli/index.ts",
    "start": "node dist/index.js",
    "benchmark": "tsx scripts/benchmark.ts",
    "clean": "rm -rf dist && npm run clean --workspaces --if-present",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "postinstall": "echo 'Gemini-Flow installed successfully'"
  },
  "keywords": [
    "gemini",
    "ai",
    "orchestration",
    "swarm",
    "agents",
    "google",
    "workspace",
    "mcp",
    "cli",
    "monorepo"
  ],
  "author": "Gemini-Flow Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gemini-flow/gemini-flow.git"
  },
  "bugs": {
    "url": "https://github.com/gemini-flow/gemini-flow/issues"
  },
  "homepage": "https://github.com/gemini-flow/gemini-flow#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "tag": "alpha",
    "provenance": true
  },
  "dependencies": {
    "@google-cloud/aiplatform": "^3.0.0",
    "@google/generative-ai": "^0.1.3",
    "@types/better-sqlite3": "^7.6.13",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "googleapis": "^154.1.0",
    "inquirer": "^9.2.0",
    "ora": "^7.0.1",
    "source-map-support": "^0.5.21",
    "sql.js": "^1.13.0",
    "sqlite3": "^5.1.6",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@types/better-sqlite3": "^7.6.11",
    "@types/inquirer": "^9.0.8",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.1.0",
    "@types/sql.js": "^1.4.9",
    "@types/sqlite3": "^3.1.8",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "babel-jest": "^30.0.5",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "node": ">=18.0.0"
  },
  "engines": {
    "node": ">=18.0.0 <=22.0.0",
    "npm": ">=8.0.0"
  },
  "optionalDependencies": {
    "better-sqlite3": "^12.2.0"
  }
}
