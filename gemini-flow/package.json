{
  "name": "gemini-flow",
  "version": "2.0.0-alpha",
  "description": "AI orchestration platform powered by Google Gemini with 64 specialized agents",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "gemini-flow": "dist/cli/index.js",
    "gf": "dist/cli/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli/index.js",
      "types": "./dist/cli/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "config/**/*",
    "README.md",
    "LICENSE"
  ],
  "workspaces": [
    "packages/*",
    "packages/agents/*",
    "packages/core/*",
    "packages/integrations/*"
  ],
  "scripts": {
    "build": "tsc && npm run build:cli",
    "build:cli": "chmod +x dist/cli/index.js",
    "build:workspaces": "npm run build --workspaces",
    "test": "jest --parallel",
    "test:workspaces": "npm run test --workspaces",
    "lint": "eslint src/**/*.ts",
    "lint:workspaces": "npm run lint --workspaces",
    "typecheck": "tsc --noEmit",
    "typecheck:workspaces": "npm run typecheck --workspaces",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "benchmark": "tsx scripts/benchmark.ts",
    "clean": "rm -rf dist && npm run clean --workspaces --if-present",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "postinstall": "npm run build:workspaces --if-present"
  },
  "keywords": [
    "gemini",
    "ai",
    "orchestration",
    "swarm",
    "agents",
    "google",
    "workspace",
    "mcp",
    "cli",
    "monorepo"
  ],
  "author": "Gemini-Flow Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gemini-flow/gemini-flow.git"
  },
  "bugs": {
    "url": "https://github.com/gemini-flow/gemini-flow/issues"
  },
  "homepage": "https://github.com/gemini-flow/gemini-flow#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "tag": "alpha",
    "provenance": true
  },
  "dependencies": {
    "@google-cloud/aiplatform": "^3.0.0",
    "@google/generative-ai": "^0.1.3",
    "better-sqlite3": "^9.0.0",
    "commander": "^11.0.0",
    "winston": "^3.11.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "inquirer": "^9.2.0",
    "googleapis": "^128.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.0",
    "@types/better-sqlite3": "^7.6.0",
    "typescript": "^5.3.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "eslint": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "tsx": "^4.0.0"
  },
  "peerDependencies": {
    "node": ">=18.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}